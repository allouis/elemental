{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "comma-dangle": [
      1,
      "always-multiline"
    ],
    "no-undef" : [
      2
    ],
    "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
    "quotes": [
      1,
      "single"
    ],
    "semi": [1, "always"]
  }
}
