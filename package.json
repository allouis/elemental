  "main": "gulpfile.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benji6/elemental.git"
  },
  "keywords": [
    "music",
    "synth",
    "web",
    "audio",
    "api"
  ],
  "author": "Ben Hall",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/benji6/elemental/issues"
  },
  "homepage": "https://github.com/benji6/elemental",
  "eslintConfig": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "rules": {
      "comma-dangle": [
