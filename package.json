{
  "name": "elemental",
  "version": "0.5.5",
  "description": "Make music in your browser!",
  "scripts": {
    "assets": "cp -r src/assets dist",
    "build": "rm -rf dist && mkdir dist && concurrent 'npm run assets' 'npm run scripts' 'gulp build' && find dist -type f -exec gzip -9 {} \\; -exec mv {}.gz {} \\;",
    "deploy": "concurrent 'npm t' 'npm run build' && aws s3 cp --recursive dist s3://elemental.audio --acl \"public-read\" --content-encoding \"gzip\"",
    "html": "html-minifier --collapse-whitespace -o dist/index.html src/index.html",
    "scripts": "browserify -t babelify src/index.js | sed 's/process.env.NODE_ENV/\"production\"/g' | uglifyjs -cm --screw-ie8 > dist/index.js",
    "serve": "live-server dist",
    "start": "concurrent 'npm run watch-assets' 'npm run watch-html' 'npm run watch-scripts' 'gulp' 'npm run serve'",
    "test": "concurrent 'standard' 'babel-node node_modules/.bin/tape 'src/**/*-spec.js' | faucet'",
    "watch-assets": "nodemon --watch src/assets -e ico,jpg,png,svg,wav --exec npm run assets",
    "watch-html": "nodemon --watch src/index.html --exec npm run html",
    "watch-scripts": "watchify -t babelify src/index.js --o dist/index.js",
    "watch-test": "nodemon --exec npm t"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benji6/elemental.git"
  },
  "keywords": [
    "elemental",
    "music",
    "synth",
    "web",
    "audio",
    "api"
  ],
  "author": "Ben Hall",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/benji6/elemental/issues"
  },
  "homepage": "https://github.com/benji6/elemental",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-eslint": "^4.1.3",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "concurrently": "^1.0.0",
    "faucet": "0.0.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.0.4",
    "html-minifier": "^1.1.1",
    "nodemon": "^1.8.1",
    "standard": "^5.4.1",
    "tape": "^4.2.2",
    "uglify-js": "^2.6.1",
    "watchify": "^3.5.0"
  },
  "dependencies": {
    "capitalize": "^1.0.0",
    "ramda": "^0.19.0",
    "random-js": "^1.0.8",
    "randomcolor": "^0.4.2",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.4",
    "rx": "^4.0.7",
    "three": "^0.73.0",
    "virtual-audio-graph": "^0.16.4"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
