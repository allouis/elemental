{
  "name": "elemental",
  "version": "0.0.0",
  "description": "Make music in your browser!",
  "scripts": {
    "test": "babel-node --stage 1 ./tests/index.js | node_modules/.bin/tap-nyan"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benji6/elemental.git"
  },
  "keywords": [
    "elemental",
    "music",
    "synth",
    "web",
    "audio",
    "api"
  ],
  "author": "Ben Hall",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/benji6/elemental/issues"
  },
  "homepage": "https://github.com/benji6/elemental",
  "eslintConfig": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "rules": {
      "comma-dangle": [
        1,
        "always-multiline"
      ],
      "quotes": [
        1,
        "single"
      ]
    }
  },
  "devDependencies": {
    "babel-eslint": "^4.0.5",
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "del": "^1.2.0",
    "eslint": "^1.0.0",
    "eslint-plugin-react": "^3.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-cdnizer": "^1.1.6",
    "gulp-connect": "^2.2.0",
    "gulp-eslint": "^1.0.0",
    "gulp-minify-css": "^1.2.0",
    "gulp-minify-html": "^1.0.4",
    "gulp-minify-inline": "^0.1.1",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.0.4",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "ramda": "^0.17.1",
    "run-sequence": "^1.1.2",
    "tap-nyan": "0.0.2",
    "tape": "^4.0.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.3.1",
    "web-audio-test-api": "^0.3.6"
  },
  "dependencies": {
    "alt": "^0.17.1",
    "capitalize": "^1.0.0",
    "random-js": "^1.0.4",
    "react": "^0.13.3",
    "react-dom": "^0.14.0-beta3",
    "virtual-audio-graph": "^0.7.11"
  }
}
